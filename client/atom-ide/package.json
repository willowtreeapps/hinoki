{
  "name": "atom-ide-brightscript",
  "version": "1.0.0-preview.1",
  "description": "BrightScript language support for Atom-IDE.",
  "main": "./lib/main",
  "repository": {
    "type": "git",
    "url": "https://github.com/willowtreeapps/brightscript-languageserver"
  },
  "author": "WillowTree Apps, Inc",
  "publisher": "willowtreeapps",
  "license": "Apache-2.0",
  "engines": {
    "atom": ">=1.21.0"
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./"
  },
  "dependencies": {
    "atom-languageclient": "0.6.4",
    "brightscript-languageserver": "file:../../server"
  },
  "enhancedScopes": [
    "source.brightscript"
  ],
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    }
  },
  "devDependencies": {
    "@types/node": "^8.0.31"
  }
