{
    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": true,
        "strict": true,
        "lib": [ "es2016" ],
        "outDir": "lib",
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitAny": true
    },
    "exclude": [ "node_modules"],
    "compileOnSave": true
}  